<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.Cursor?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/23" 
      xmlns:fx="http://javafx.com/fxml/1" 
      fx:controller="Chaves.ProjetoLicitacoes.controller.TelaEditarFornecedorController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="626.0" prefWidth="690.0">
            <children>
                <Text layoutX="220.0" layoutY="51.0" strokeType="OUTSIDE" text="Editar fornecedor">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Text>

                <TextArea fx:id="inputNome" layoutX="220.0" layoutY="74.0" 
                         prefHeight="18.0" prefWidth="200.0" promptText="Nome" />
                         
                <TextArea fx:id="inputCnpj" layoutX="220.0" layoutY="127.0" 
                         prefHeight="18.0" prefWidth="200.0" promptText="CNPJ" />
                         
                <TextArea fx:id="inputEmail" layoutX="220.0" layoutY="178.0" 
                         prefHeight="18.0" prefWidth="200.0" promptText="E-mail" />
                         
                <TextArea fx:id="inputTelefone" layoutX="220.0" layoutY="229.0" 
                         prefHeight="18.0" prefWidth="200.0" promptText="Telefone" />
                         
                <Label fx:id="labelMensagem" layoutX="220.0" layoutY="296.0" 
                      prefHeight="37.0" prefWidth="200.0" textFill="RED" />

                <Button fx:id="btnVoltar" layoutX="235.0" layoutY="354.0" 
                        onAction="#voltar" text="Voltar">
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>

                <Button fx:id="btnSalvar" layoutX="336.0" layoutY="354.0" 
                        onAction="#salvar" text="Salvar">
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
            </children>
        </AnchorPane>
    </children>
</VBox>