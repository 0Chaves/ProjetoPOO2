<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Chaves.ProjetoLicitacoes.controller.TelaFornecedorController">
  <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="626.0" prefWidth="690.0">
         <children>
            <Button fx:id="botao_enviar" blendMode="DIFFERENCE" cache="true" cacheHint="SCALE_AND_ROTATE" layoutX="336.0" layoutY="354.0" onAction="#enviar" opacity="0.39" prefHeight="32.0" prefWidth="64.0" text="Enviar" textOverrun="CENTER_WORD_ELLIPSIS">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Text layoutX="220.0" layoutY="51.0" strokeDashOffset="19.0" strokeMiterLimit="19.0" strokeType="OUTSIDE" strokeWidth="15.0" text="Inserir novo fornecedor">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Text>
            <Button fx:id="botao_voltar" blendMode="DIFFERENCE" cache="true" cacheHint="SCALE_AND_ROTATE" layoutX="235.0" layoutY="354.0" onAction="#voltar" opacity="0.39" prefHeight="32.0" prefWidth="64.0" text="Voltar" textOverrun="CENTER_WORD_ELLIPSIS">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <TextArea fx:id="inputNome" layoutX="220.0" layoutY="74.0" prefHeight="18.0" prefWidth="200.0" promptText="Nome" />
            <TextArea fx:id="inputCnpj" layoutX="220.0" layoutY="127.0" prefHeight="18.0" prefWidth="200.0" promptText="Cnpj" />
            <TextArea fx:id="inputEmail" layoutX="220.0" layoutY="178.0" prefHeight="18.0" prefWidth="200.0" promptText="E-mail" />
            <TextArea fx:id="inputTelefone" layoutX="220.0" layoutY="229.0" prefHeight="18.0" prefWidth="200.0" promptText="Telefone" />
            <Label layoutX="220.0" layoutY="296.0" prefHeight="37.0" prefWidth="200.0" />
         </children>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </AnchorPane>
  </children>
</VBox>
