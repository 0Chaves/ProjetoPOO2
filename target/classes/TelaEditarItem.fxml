<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.Cursor?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/23" 
      xmlns:fx="http://javafx.com/fxml/1" 
      fx:controller="Chaves.ProjetoLicitacoes.controller.TelaEditarItemController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="626.0" prefWidth="690.0">
            <children>
                <Text layoutX="220.0" layoutY="51.0" strokeType="OUTSIDE" text="Editar Item">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Text>

                <TextArea fx:id="inputDescricao" layoutX="115.0" layoutY="127.0" 
                         prefHeight="64.0" prefWidth="411.0" promptText="Descrição" />
                         
                <TextArea fx:id="inputPregao" layoutX="115.0" layoutY="200.0" 
                         prefHeight="18.0" prefWidth="200.0" promptText="Pregão (XX/YYYY)" />
                         
                <TextArea fx:id="inputValorUnitario" layoutX="115.0" layoutY="249.0" 
                         prefHeight="18.0" prefWidth="200.0" promptText="Valor Unitário" />
                         
                <TextArea fx:id="inputQuantidadeMaxima" layoutX="326.0" layoutY="200.0" 
                         prefHeight="18.0" prefWidth="200.0" promptText="Quantidade Máxima" />

                <ChoiceBox fx:id="choiceFornecedor" layoutX="326.0" layoutY="249.0" 
                          prefHeight="38.0" prefWidth="200.0" />
                          
                <Label fx:id="labelMensagem" layoutX="115.0" layoutY="300.0" 
                      prefWidth="411.0" textFill="RED" />

                <HBox alignment="CENTER" spacing="20.0" layoutX="220.0" layoutY="350.0">
                    <Button fx:id="btnVoltar" onAction="#voltar" text="Voltar" />
                    <Button fx:id="btnSalvar" onAction="#salvar" text="Salvar" />
                </HBox>
            </children>
        </AnchorPane>
    </children>
</VBox>